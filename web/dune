(executable
  (name mttweb)
  (libraries js_of_ocaml-lwt base mtt)
  (modes byte)
  (preprocess (pps js_of_ocaml-ppx ppx_let ppx_sexp_conv ppx_string_interpolation))
  (modules_without_implementation ace_types))

(rule
  (targets mttweb.js)
  (action
    (run %{bin:js_of_ocaml}
         --source-map %{dep:mttweb.bc} -o %{targets} --pretty)))

(alias
  (name default)
  (deps mttweb.js index.html))
